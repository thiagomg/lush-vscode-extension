{
  "name": "LuSH",
  "scopeName": "source.lush",
  "patterns": [
    {
      "name": "support.function.env.lush",
      "match": "\\b(env\\.(pushd|popd|chdir|pwd|get|set|del|print))\\b"
    },
    {
      "name": "support.function.files.lush",
      "match": "\\b(files\\.(zip|unzip|compress|decompress))\\b"
    },
    {
      "name": "support.function.fs.lush",
      "match": "\\b(fs\\.(ls|mkdir|rmdir|copy|move|rm|exists))\\b"
    },
    {
      "name": "support.function.os.lush",
      "match": "\\b(os\\.(name|proc_names|proc_exes|exec))\\b"
    },
    {
      "name": "string.quoted.double.lush",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.lush",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "comment.line.double-dash.lush",
      "match": "--.*$"
    },
    {
      "name": "constant.numeric.lush",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "keyword.control.lush",
      "match": "\\b(if|else|elseif|end|function|return|for|while|do|local|in|then|repeat|until|break|and|or|not)\\b"
    },
    {
      "name": "variable.other.lush",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
    }
  ],
  "repository": {},
  "scopeName": "source.lush"
}
